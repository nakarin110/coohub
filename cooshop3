local PartyAddButton = PartyTab:CreateButton({
   Name = "‡πÄ‡∏û‡∏¥‡πà‡∏°‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô‡πÉ‡∏ô‡∏õ‡∏≤‡∏£‡πå‡∏ï‡∏µ‡πâ",
   Callback = function()
      if CurrentPartyInput ~= "" then
         local playerExists = false
         for _, player in pairs(game.Players:GetPlayers()) do
            if player.Name:lower() == CurrentPartyInput:lower() then
               playerExists = true
               CurrentPartyInput = player.Name
               break
            end
         end
         
         local alreadyInParty = false
         for _, memberName in pairs(PartyMembers) do
            if memberName:lower() == CurrentPartyInput:lower() then
               alreadyInParty = true
               break
            end
         end
         
         if not alreadyInParty then
            table.insert(PartyMembers, CurrentPartyInput)
            Rayfield:Notify({
               Title = "‡∏£‡∏∞‡∏ö‡∏ö‡∏õ‡∏≤‡∏£‡πå‡∏ï‡∏µ‡πâ",
               Content = "‡πÄ‡∏û‡∏¥‡πà‡∏° " .. CurrentPartyInput .. " ‡πÄ‡∏Ç‡πâ‡∏≤‡∏õ‡∏≤‡∏£‡πå‡∏ï‡∏µ‡πâ‡πÅ‡∏•‡πâ‡∏ß" .. (playerExists and "" or " (‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô‡∏≠‡∏≤‡∏à‡πÑ‡∏°‡πà‡πÑ‡∏î‡πâ‡∏≠‡∏¢‡∏π‡πà‡πÉ‡∏ô‡πÄ‡∏Å‡∏°)"),
               Duration = 3,
            })
            
            updatePartyList()
         else
            Rayfield:Notify({
               Title = "‡∏£‡∏∞‡∏ö‡∏ö‡∏õ‡∏≤‡∏£‡πå‡∏ï‡∏µ‡πâ",
               Content = CurrentPartyInput .. " ‡∏≠‡∏¢‡∏π‡πà‡πÉ‡∏ô‡∏õ‡∏≤‡∏£‡πå‡∏ï‡∏µ‡πâ‡∏≠‡∏¢‡∏π‡πà‡πÅ‡∏•‡πâ‡∏ß",
               Duration = 3,
            })
         end
      else
         Rayfield:Notify({
            Title = "‡∏£‡∏∞‡∏ö‡∏ö‡∏õ‡∏≤‡∏£‡πå‡∏ï‡∏µ‡πâ",
            Content = "‡∏Å‡∏£‡∏∏‡∏ì‡∏≤‡πÉ‡∏™‡πà‡∏ä‡∏∑‡πà‡∏≠‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô‡∏Å‡πà‡∏≠‡∏ô",
            Duration = 3,
         })
      end
   end,
})

local PartyListLabel = PartyTab:CreateLabel("‡∏™‡∏°‡∏≤‡∏ä‡∏¥‡∏Å‡∏õ‡∏≤‡∏£‡πå‡∏ï‡∏µ‡πâ: ‡πÑ‡∏°‡πà‡∏°‡∏µ‡∏™‡∏°‡∏≤‡∏ä‡∏¥‡∏Å")

function updatePartyList()
    if #PartyMembers == 0 then
        PartyListLabel:Set("‡∏™‡∏°‡∏≤‡∏ä‡∏¥‡∏Å‡∏õ‡∏≤‡∏£‡πå‡∏ï‡∏µ‡πâ: ‡πÑ‡∏°‡πà‡∏°‡∏µ‡∏™‡∏°‡∏≤‡∏ä‡∏¥‡∏Å")
    else
        local membersList = "‡∏™‡∏°‡∏≤‡∏ä‡∏¥‡∏Å‡∏õ‡∏≤‡∏£‡πå‡∏ï‡∏µ‡πâ: " .. table.concat(PartyMembers, ", ")
        PartyListLabel:Set(membersList)
    end
end

local PartyClearButton = PartyTab:CreateButton({
   Name = "‡∏•‡∏ö‡∏™‡∏°‡∏≤‡∏ä‡∏¥‡∏Å‡∏ó‡∏±‡πâ‡∏á‡∏´‡∏°‡∏î",
   Callback = function()
      PartyMembers = {}
      updatePartyList()
      Rayfield:Notify({
         Title = "‡∏£‡∏∞‡∏ö‡∏ö‡∏õ‡∏≤‡∏£‡πå‡∏ï‡∏µ‡πâ",
         Content = "‡∏•‡∏ö‡∏™‡∏°‡∏≤‡∏ä‡∏¥‡∏Å‡∏ó‡∏±‡πâ‡∏á‡∏´‡∏°‡∏î‡∏≠‡∏≠‡∏Å‡∏à‡∏≤‡∏Å‡∏õ‡∏≤‡∏£‡πå‡∏ï‡∏µ‡πâ‡πÅ‡∏•‡πâ‡∏ß",
         Duration = 3,
      })
   end,
})

local PartyRemoveInput = PartyTab:CreateInput({
   Name = "‡∏•‡∏ö‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô‡∏≠‡∏≠‡∏Å‡∏à‡∏≤‡∏Å‡∏õ‡∏≤‡∏£‡πå‡∏ï‡∏µ‡πâ",
   PlaceholderText = "‡πÉ‡∏™‡πà‡∏ä‡∏∑‡πà‡∏≠‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô‡∏ó‡∏µ‡πà‡∏ï‡πâ‡∏≠‡∏á‡∏Å‡∏≤‡∏£‡∏•‡∏ö‡∏≠‡∏≠‡∏Å‡∏à‡∏≤‡∏Å‡∏õ‡∏≤‡∏£‡πå‡∏ï‡∏µ‡πâ",
   RemoveTextAfterFocusLost = false,
   Callback = function(Text)
      RemovePartyInput = Text
   end,
})

local PartyRemoveButton = PartyTab:CreateButton({
   Name = "‡∏•‡∏ö‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô",
   Callback = function()
      if RemovePartyInput ~= "" then
         local removed = false
         for i, memberName in pairs(PartyMembers) do
            if memberName:lower() == RemovePartyInput:lower() then
               table.remove(PartyMembers, i)
               removed = true
               break
            end
         end
         
         if removed then
            updatePartyList()
            Rayfield:Notify({
               Title = "‡∏£‡∏∞‡∏ö‡∏ö‡∏õ‡∏≤‡∏£‡πå‡∏ï‡∏µ‡πâ",
               Content = "‡∏•‡∏ö " .. RemovePartyInput .. " ‡∏≠‡∏≠‡∏Å‡∏à‡∏≤‡∏Å‡∏õ‡∏≤‡∏£‡πå‡∏ï‡∏µ‡πâ‡πÅ‡∏•‡πâ‡∏ß",
               Duration = 3,
            })
         else
            Rayfield:Notify({
               Title = "‡∏£‡∏∞‡∏ö‡∏ö‡∏õ‡∏≤‡∏£‡πå‡∏ï‡∏µ‡πâ",
               Content = "‡πÑ‡∏°‡πà‡∏û‡∏ö " .. RemovePartyInput .. " ‡πÉ‡∏ô‡∏õ‡∏≤‡∏£‡πå‡∏ï‡∏µ‡πâ",
               Duration = 3,
            })
         end
      else
         Rayfield:Notify({
            Title = "‡∏£‡∏∞‡∏ö‡∏ö‡∏õ‡∏≤‡∏£‡πå‡∏ï‡∏µ‡πâ",
            Content = "‡∏Å‡∏£‡∏∏‡∏ì‡∏≤‡πÉ‡∏™‡πà‡∏ä‡∏∑‡πà‡∏≠‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô‡∏Å‡πà‡∏≠‡∏ô",
            Duration = 3,
         })
      end
   end,
})

local nameHidingHeartbeat = game:GetService("RunService").Heartbeat:Connect(function()
    if HideMyName then
        if game.Players.LocalPlayer.Character then
            for _, gui in pairs(game.Players.LocalPlayer.Character:GetDescendants()) do
                if (gui:IsA("BillboardGui") or gui:IsA("SurfaceGui")) and
                   (gui.Name:match("PlayerName") or gui.Name:match("NameTag") or gui.Name:match("Display") or gui.Name:match("Head")) then
                    if gui.Enabled then
                        gui.Enabled = false
                    end
                end
            end
        end
    end
end)

game.Players.LocalPlayer.CharacterRemoving:Connect(function()
    if nameHidingHeartbeat then
        nameHidingHeartbeat:Disconnect()
    end
end)
